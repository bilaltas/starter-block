{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/starter-block",
	"version": "0.1.0",
	"title": "Starter Block",
	"category": "text",
	"icon": "flag",
	"description": "A base editable component for unique blocks.",
	"attributes": {
		"title": {
			"type": "string",
			"source": "html",
			"selector": "h2",
			"default": "Default Title"
		},
		"description": {
			"type": "string",
			"source": "text",
			"selector": "span.description"
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "bilal-tas",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
